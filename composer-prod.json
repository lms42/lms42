{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        { "type": "git", "url": "git@bitbucket.org:4xxi/smirikadminbundle.git" },
        { "type": "git", "url": "git@bitbucket.org:4xxi/smirikcontentbundle.git" },
        { "type": "git", "url": "git@bitbucket.org:4xxi/smirikquizbundle.git" },
        { "type": "git", "url": "git@bitbucket.org:4xxi/smirikpropeladminbundle.git" },
        { "type": "git", "url": "git@bitbucket.org:4xxi/smirikcoursebundle.git" }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.3",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.0.*",
        "twig/extensions": "1.0.x-dev",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.1.*",
        "symfony/monolog-bundle": "2.1.*",
        "sensio/distribution-bundle": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*",
        "sensio/generator-bundle": "2.1.*",
        "jms/security-extra-bundle": "1.2.*",
        "jms/di-extra-bundle": "1.1.*",
        "propel/propel-bundle": "1.1.*",
        "friendsofsymfony/user-bundle": "dev-master",
        "willdurand/propel-typehintable-behavior": "dev-master",
        "imagine/Imagine": "v0.4.1",
        "avalanche123/imagine-bundle": "dev-master",
        "smirik/bootstrap-bundle": "dev-master",
        "knplabs/knp-menu": "2.0.x-dev@dev",
        "knplabs/knp-menu-bundle": "dev-master",
        "kriswallsmith/assetic": "1.1.*@dev",
        "smirik/content-bundle": "dev-master",
        "smirik/admin-bundle": "dev-master",
        "smirik/propel-admin-bundle": "dev-master",
        "smirik/course-bundle": "dev-master",
        "smirik/quiz-bundle": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
